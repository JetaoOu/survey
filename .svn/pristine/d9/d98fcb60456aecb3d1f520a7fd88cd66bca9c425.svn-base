<view><view class="uni-padding-wrap uni-common-mt"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]],['reset',[['formReset',['$event']]]]]}}" bindsubmit="__e" bindreset="__e"><view class="form-title"><label class="form-title-text">{{form.title}}</label></view><block wx:for="{{form.list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{item.radio}}"><view><view class="uni-list"><view class="example-title">{{item.name}}</view><radio-group name="{{index}}" data-event-opts="{{[['change',[['radioChange',['$event',index]]]]]}}" bindchange="__e"><block wx:for="{{item.ss}}" wx:for-item="item" wx:for-index="index1" wx:key="value"><label class="uni-list-cell uni-list-cell-pd"><view><radio value="{{item.value}}" checked="{{index1===form.list[index].current}}"></radio></view><view>{{item.name}}</view></label></block></radio-group></view></view></block><block wx:if="{{item.checkbox}}"><view><view class="example-title">{{item.name}}</view><view class="uni-list"><checkbox-group name="{{index}}" data-event-opts="{{[['change',[['checkboxChange',['$event',index]]]]]}}" bindchange="__e"><block wx:for="{{item.ss}}" wx:for-item="item" wx:for-index="__i0__" wx:key="value"><label class="uni-list-cell uni-list-cell-pd"><view><checkbox value="{{item.value}}" checked="{{item.checked}}"></checkbox></view><view>{{item.name}}</view></label></block></checkbox-group></view></view></block><block wx:if="{{item.rate}}"><view><view class="uni-list"><view class="example-title">{{item.name}}</view><view class="example-body"><uni-rate vue-id="{{'1-'+index}}" value="{{item.value}}" index="{{index}}" margin="{{10}}" max="{{item.max}}" data-event-opts="{{[['^change',[['onChange']]]]}}" bind:change="__e" bind:__l="__l"></uni-rate></view><view hidden="{{!(false)}}"><input type="text" name="{{index}}" value="{{item.value}}"/></view><view class="rate-vname">{{item.vname[form.list[index].value-1]}}</view></view></view></block></view></block><view class="uni-btn-v"><button formType="submit">Submit</button><button type="default" formType="reset">Reset</button></view></form></view></view>