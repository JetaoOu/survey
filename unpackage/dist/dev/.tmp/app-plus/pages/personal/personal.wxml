<block wx:if="{{onlogin}}"><view><cmd-page-body vue-id="1" type="top" bind:__l="__l" vue-slots="{{['default']}}"><cmd-transition vue-id="{{('2')+','+('1')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><view><cmd-cel-item vue-id="{{('3')+','+('2')}}" title="头像" slot-right bind:__l="__l" vue-slots="{{['default']}}"><cmd-avatar vue-id="{{('4')+','+('3')}}" src="{{user.avatar}}" bind:__l="__l"></cmd-avatar></cmd-cel-item><cmd-cel-item vue-id="{{('5')+','+('2')}}" title="昵称" addon="{{user.nickname}}" bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('6')+','+('2')}}" title="姓名" addon="{{user.name}}" bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('7')+','+('2')}}" title="联系方式" addon="{{user.phone}}" bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('8')+','+('2')}}" title="我的地址" addon="{{user.address}}" bind:__l="__l"></cmd-cel-item><cmd-cel-item vue-id="{{('9')+','+('2')}}" title="信息设置" arrow data-event-opts="{{[['^click',[['fnClick',['info']]]]]}}" bind:click="__e" bind:__l="__l"></cmd-cel-item></view></cmd-transition></cmd-page-body></view></block><block wx:else><view><button type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登陆</button></view></block>